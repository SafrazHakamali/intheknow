  
    <div class="jumbotron">
      <h1>In the Know Movie Reviews</h1>
      <% if !user_signed_in? %>
      <p><%= link_to "Sign Up To Write A Review", new_user_registration_path, class: "btn btn-primary btn-lg" %></p>
      <% end %>
    </div>
  

  <div class="row">
    <% @movies.each do |movie| %>
      <div class="col-sm-6 col-md-4 col-lg-2">
        <div class="thumbnail">
          <%= link_to (image_tag movie.image, class: 'image'), movie %>
        </div>
      </div>
    <% end %>
  </div>